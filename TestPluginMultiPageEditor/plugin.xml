<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Sample Multi-page Editor"
            extensions="mpe"
            icon="icons/sample.gif"
            contributorClass="testplugineditor.editors.MultiPageEditorContributor"
            class="testplugineditor.editors.MultiPageEditor"
            id="testplugineditor.editors.MultiPageEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="Sample Wizards"
            id="TestPluginEditor">
      </category>
      <wizard
            name="Multi-page Editor file"
            icon="icons/sample.gif"
            category="TestPluginEditor"
            class="testplugineditor.wizards.SampleNewWizard"
            id="testplugineditor.wizards.SampleNewWizard">
      </wizard>
   </extension>

</plugin>
